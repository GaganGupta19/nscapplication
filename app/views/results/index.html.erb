<p id="notice"><%= notice %></p>

<h1>Listing Results</h1>

<table class="CSSTableGenerator">
  <thead>
    <tr>
      <th>Team ID</th>
      <th>Event ID</th>
      <th>Round</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td>T-<%= result.team_id %></td>
        <td><%= result.event_id %></td>
        <td><%= result.round %></td>
        <td><%= link_to 'Show', result %></td>
        <td><%= link_to 'Edit', edit_result_path(result) %></td>
        <% if coordinator_signed_in? %>
        <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
